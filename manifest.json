{
  "manifest_version": 3,
  "name": "Prospectors Grand Land Tools",
  "version": "1.0",
  "description": "Tools for Prospectors Grand Land",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://grandland.prospectors.io/*"],
      "js": ["content.js"]
    }
  ]
}
